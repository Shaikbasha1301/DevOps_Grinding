EGRESS


ApiVersion: networking.k8s.io/v1
kind: NetworkPolicy
metadata:
	name : egress-App1
spec:
	podSelector:
		matchLabels:
			app: swiggy
	policyTypes:	
		- Egress
	egress:
		- to:
			- podSelector:
				matchLabels:
					app: client
					
					
