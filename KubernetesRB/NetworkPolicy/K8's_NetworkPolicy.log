Creating Pods

apiVersion: v1
kind: Pod
metadata: 
	name: swiggy, client, zomato, uber
	labels:
		app: swiggyapp, clientapp, zomatoapp, uberapp
spec:
	containers:
		- name: Mycont-1
          image: ngnix
          ports:
			containerPort: 80
		


