INGRESS 

ApiVersion: networking.k8s.io/v1
kind: NetworkPolicy
metadata:
	name : ingress-App
spec:
	podSelector:
	   matchLabels:
	      app: client
	policyTypes:	
		- Ingress
	ingress:
		- from:
			- podSelector:
				matchLabels:
					app: swiggy
					
